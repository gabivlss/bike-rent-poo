// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  name     String
  email    String @unique
  password String
}

model Bike{
  name String
  type String
  bodySize Int
  maxLoad Int
  rate Int
  description String
  ratings Int
  imageUrls String
  available Boolean
  latitude String
  longitude String
  id String @id @default(uuid())
}
